<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<meta http-equiv="X-UA-Compatible" content="IE=9"/>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/images/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/Content/AllCorescripts")
    @Scripts.Render("~/Content/NotyScripts")
    
</head>
<body>
    <table class="LayoutHeader">
        <tr>
            <td class="Header" id="HeaderRow">
            <script>
                $('#HeaderRow').css("visibility", "hidden");
            </script>
                <table>
                    <tr>
                        <td class="Logo" style="width:2%">
                            <p class="site-title">
                            <a href="~/Home/Account/Index"></a>

                           </p>
                        </td>

                            

                        <td class="SiteMap" style="width:70%">
                            <div id="page-wrap" style="width:100%">


                                @Html.Partial("_SetMainMenuStrip")


                            </div>
                        </td>

                        
                        <td class="Cred" style="width:25%">
                            @Html.Partial("_LoginPartial")
                        </td>
                    </tr>
                </table>
                <script>
                    $('#HeaderRow').css("visibility", "visible");
            </script>

            </td>
        </tr>

        <tr>
            <td class="body-content">

                <table class="Layout" cellpadding="0" cellspacing="0">
                    <tr>
                        <td class="pagecontent">
                            <div style="padding: 0px;padding-top:5px; margin: 10px;margin-top:5px;">
                                @RenderBody()
                            </div>
                                 
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    
        <tr>
            <td class="footer">
                <div class="footerbody">
                    <div class="textAlignRight" style="padding-right: 5px;font-size:11px; vertical-align:middle;">
                        <b>GPS STATE 4 CYCLE 5 </b></div>
                </div>
            </td>
        </tr>
     
    </table>
    <div class="cbox">
    </div>
    @RenderSection("scripts", required: false)
   
</body>
</html>
